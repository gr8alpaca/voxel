; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="voxel"
run/main_scene="uid://ds33v6r6004te"
run/main_loop_type="Master"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

VoxelPool="*res://autoloads/voxel_pool.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/unreachable_code=0
gdscript/warnings/standalone_expression=0
gdscript/warnings/standalone_ternary=0
gdscript/warnings/incompatible_ternary=0
gdscript/warnings/unsafe_void_return=0
gdscript/warnings/static_called_on_instance=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0
gdscript/warnings/int_as_enum_without_cast=0
gdscript/warnings/int_as_enum_without_match=0
gdscript/warnings/enum_variable_without_default=0
gdscript/warnings/empty_file=0

[file_customization]

folder_colors={
"res://autoloads/": "teal",
"res://icons/": "pink",
"res://scripts/": "yellow"
}

[physics]

2d/physics_engine="Dummy"
3d/physics_engine="Dummy"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
